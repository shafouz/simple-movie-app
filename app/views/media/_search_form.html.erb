<div class="form">
  <%= turbo_frame_tag "search_form" do %>
    <%= form_with url: "/search", data: { select_target: "searchForm" } do |form| %>
      <%= form.text_field :query %>
      <%= form.submit "Search", class: "btn btn-primary" %>
    <% end %>
  <% end %>
  <%= turbo_frame_tag "results" do %>
  <% end %>
</div>
