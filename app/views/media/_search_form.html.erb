<div class="form">
  <%= turbo_frame_tag "search_form" do %>
    <%= form_with url: "/search" do |form| %>
      <%= form.text_field :query %>
      <%= form.submit "Search", class: "py-2 px-4 font-semibold rounded-lg shadow-md text-white bg-blue-500 hover:bg-blue-700 focus:outline-none" %>
    <% end %>
  <% end %>
</div>
