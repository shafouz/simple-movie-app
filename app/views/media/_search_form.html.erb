<div class="form">
  <%= turbo_frame_tag "search_form" do %>
    <%= form_with url: "/search" do |form| %>
      <%= form.text_field :query, class: form_control, placeholder: "Pesquise por filme, serie ou pessoa..." %>
      <%= form.submit "Search", class: btn_primary %>
    <% end %>
  <% end %>
</div>
